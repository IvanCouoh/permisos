<%- include ../layout/backOffice/header.ejs %>
<div class="container">
    <form action="../actualizar/" method="POST" enctype="multipart/form-data">
      <input type="hidden" name="id" value="<%= usuarios.id %> ">
      <div class="form-group">
        <label for="my-input">Nombre:</label>
        <input id="my-input" class="form-control" type="text" name="nombre" value="<%= usuarios.nombre %> " required/>
      </div>
      <div class="form-group">
        <label for="my-input">Email:</label>
        <input id="my-input" class="form-control" type="email" name="email" value="<%= usuarios.email %> " required/>
      </div>
      <div class="form-group">
        <label for="my-input">ContraseÃ±a:</label>
        <input id="my-input" class="form-control" type="text" name="pass" value="<%= usuarios.pass %> " required/>
      </div>
      <div class="form-group">
        <label for="my-input">Imagen:</label>
        <img class="img-thumbnail" src="../../images/<%= usuarios.imagen %>" width="100" alt="">
        <input id="my-input" class="form-control-file" type="file" name="imagen" required/>
      </div>
      <button class="btn btn-primary" type="submit">Guardar</button>
    </form>
</div>
